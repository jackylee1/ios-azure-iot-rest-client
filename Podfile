project 'GeoLocation.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'GeoLocation' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for GeoLocation
  pod 'MicrosoftAzureMobile', '~> 3.2.0'
  pod 'IDZSwiftCommonCrypto', '~> 0.9.0'
  pod 'Mapbox-iOS-SDK', '~> 3.3.7'
  pod 'MapboxDirections.swift', :git => 'https://github.com/mapbox/MapboxDirections.swift.git', :tag => 'swift3'
  pod 'MapboxGeocoder.swift', :git => 'https://github.com/mapbox/MapboxGeocoder.swift.git', :tag => 'swift3'
  pod 'Polyline', '~> 4.0'


  target 'GeoLocationTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GeoLocationUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
